{
  "name": "base node app",
  "version": "1.0.0",
  "description": "Base node app  in typescript with jest, eslint and prettier.",
  "scripts": {
    "local": "dotenv-run-script .env --",
    "test": "jest",
    "check": "tsc --noEmit && npm run lint && prettier --check \"**/*.{ts,json,md,cjs}\"",
    "lint": "tsc --noEmit && eslint . --ext .ts",
    "format": "prettier --write",
    "dev": "nodemon src/index.ts",
    "start": "npm run local dev"
  },
  "engines": {
    "node": ">= 16",
    "npm": ">= 7"
  },
  "author": {
    "name": "commercetools Professional Services",
    "email": "harm.meijer@commercetools.com"
  },
  "license": "MIT",
  "dependencies": {
    "@commercetools/platform-sdk": "^7.19.0",
    "@commercetools/sdk-client-v2": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.13",
    "@types/node": "20.16.10",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "dotenv-run-script": "0.4.1",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-deprecation": "2.0.0",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.7",
    "nodemon": "3.1.7",
    "prettier": "2.8.8",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "4.9.5"
  }
}
